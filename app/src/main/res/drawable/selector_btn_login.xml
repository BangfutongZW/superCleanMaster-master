<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#ff34619d" />
            <stroke android:width="1.0dip" android:color="#ff34619d" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#ffa1a1a1" />
            <stroke android:width="1.0dip" android:color="#ffa1a1a1" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#ff4883d2" />
            <stroke android:width="1.0dip" android:color="#ff4883d2" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>